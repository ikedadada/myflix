# direnv example for dev
# Copy to .envrc and run `direnv allow`.
# Secrets are loaded via env vars (not committed).

export TF_VAR_cloudflare_api_token="replace-with-token"
export TF_VAR_cloudflare_account_id="replace-with-account-id"
export TF_VAR_cloudflare_zone_id="example.com"
export TF_VAR_project_slug="myflix"
export TF_VAR_root_domain="example.com"
export TF_VAR_allowed_emails='["dev@example.com"]'
export AWS_ACCESS_KEY_ID="example id"
export AWS_SECRET_ACCESS_KEY="example secrets"
