# direnv example for prod
# Copy to .envrc and run `direnv allow`.
# Secrets are loaded via env vars (not committed).

export TF_VAR_cloudflare_api_token="replace-with-token"
export TF_VAR_cloudflare_account_id="replace-with-account-id"
export TF_VAR_project_slug="myflix"
export TF_VAR_access_domain="example.cloudflareaccess.com"
export TF_VAR_allowed_emails='["prod@example.com"]'
